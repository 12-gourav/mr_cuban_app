{
  "expo": {
    "name": "MR Cuban",
    "scheme": "mrcuban",
    "slug": "mrcuban",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#FF231F7C"
      },
      "package": "com.gourav_bajpai1212.mrcuban"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fc9e2bc0-8eb2-4751-8931-eedb9e5afc84"
      }
    }
  }
}

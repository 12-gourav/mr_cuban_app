{
  "expo": {
    "name": "MR Cuban",
    "scheme": "mrcuban",
    "slug": "mrcuban",
    "version": "9.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#FF231F7C"
      },
      "package": "com.gourav_bajpaizxcv.mrcuban"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f41f0d73-3602-48f7-912a-f60d59cabe7e"
      }
    }
  }
}
